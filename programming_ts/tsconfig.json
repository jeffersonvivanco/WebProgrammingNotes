{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6", "es5", "dom"
        ],
        "target": "es5",
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules"
    ]
}